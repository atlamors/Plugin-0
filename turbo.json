{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "@atla/medusa-store-filters#dev": {
            "cache": false,
            "persistent": true
        },
        "@atla/medusa-store-filters#ready": {
            "cache": false
        },
        "@atla/plugin-0#dev": {
            "cache": false,
            "persistent": true
        },
        "@atla/plugin-0#ready": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "test": {
            "dependsOn": [
                "build"
            ]
        },
        "typecheck": {},
        "lint": {}
    }
}